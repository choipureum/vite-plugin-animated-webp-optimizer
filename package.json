{
  "name": "vite-plugin-animated-webp-optimizer",
  "version": "1.0.20",
  "description": "Vite plugin for optimizing animated WebP files with Sharp and webpmux",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:html": "jest --coverage --coverageReporters=html",
    "test:coverage:codecov": "jest --coverage --coverageReporters=lcov && codecov || echo 'Codecov upload failed, but tests passed'",
    "prepare": "husky install",
    "webp:optimize": "node scripts/optimize-webp.js",
    "webp:optimize:watch": "node scripts/optimize-webp.js --watch",
    "webp:optimize:fast": "node scripts/optimize-webp.js --fast",
    "webp:clean": "node scripts/clean-optimized.js",
    "prebuild": "npm run webp:optimize:fast",
    "build:fast": "npm run webp:clean && npm run build"
  },
  "keywords": [
    "vite",
    "plugin",
    "webp",
    "animated",
    "optimization",
    "sharp",
    "webpmux"
  ],
  "author": "poo1994@ncsoft.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/choipureum/vite-plugin-animated-webp-optimizer"
  },
  "homepage": "https://github.com/choipureum/vite-plugin-animated-webp-optimizer",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "codecov": "^3.8.2",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "sharp": "^0.32.0"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  }
}
