{
  "name": "vite-plugin-animated-webp-optimizer",
  "version": "1.0.0",
  "description": "Vite plugin for optimizing animated WebP files with Sharp and webpmux",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:html": "jest --coverage --coverageReporters=html",
    "test:coverage:codecov": "jest --coverage --coverageReporters=lcov && codecov || echo 'Codecov upload failed, but tests passed'",
    "prepare": "husky install"
  },
  "keywords": [
    "vite",
    "plugin",
    "webp",
    "animated",
    "optimization",
    "sharp",
    "webpmux"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "codecov": "^3.8.2",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "execa": "^7.0.0",
    "sharp": "^0.32.0"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  }
}
